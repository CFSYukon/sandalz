<p id="notice"><%= notice %></p>

<h1>Inventory Serial Numbers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inventory_serial_numbers.each do |inventory_serial_number| %>
      <tr>
        <td><%= link_to 'Show', inventory_serial_number %></td>
        <td><%= link_to 'Edit', edit_inventory_serial_number_path(inventory_serial_number) %></td>
        <td><%= link_to 'Destroy', inventory_serial_number, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inventory Serial Number', new_inventory_serial_number_path %>
