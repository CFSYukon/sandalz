<p id="notice"><%= notice %></p>

<h1>Inventory SKUs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inventory_skus.each do |inventory_sku| %>
      <tr>
        <td><%= inventory_sku.descriptive_id %></td>
        <td><%= link_to 'Show', inventory_sku %></td>
        <td><%= link_to 'Edit', edit_inventory_sku_path(inventory_sku) %></td>
        <td><%= link_to 'Destroy', inventory_sku, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inventory Sku', new_inventory_sku_path %>
